{
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.view.pdf.tab.useNewGroup": true,
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.latex.recipes": [
      {
        "name": "latexmk ðŸ”ƒ",
        "tools": [
          "latexmk"
        ]
      },
      {
        "name": "pdflatex âžž bibtex âžž pdflatexâ€ŠÃ—â€Š2",
        "tools": [
          "pdflatex",
          "bibtex",
          "pdflatex",
          "pdflatex"
        ]
      },
      {
        "name": "arara",
        "tools": [
          "arara"
        ]
      }
    ],
    "latex-workshop.latex.tools": [
      {
        "name": "latexmk",
        "command": "latexmk",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "-pdf",
          "-outdir=%OUTDIR%",
          "%DOC%"
        ],
        "env": {}
      },
      {
        "name": "pdflatex",
        "command": "pdflatex",
        "args": [
          "-synctex=1",
          "-interaction=nonstopmode",
          "-file-line-error",
          "%DOC%"
        ],
        "env": {}
      },
      {
        "name": "bibtex",
        "command": "bibtex",
        "args": [
          "%DOCFILE%"
        ],
        "env": {}
      },
      {
        "name": "arara",
        "command": "arara",
        "args": [
          "--language=it",
          "--log",
          "--verbose",
          "%DOCFILE%"
        ],
        "env": {}
      }
    ],
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.chktex.delay": 1000,
    "latex-workshop.chktex.run": "onType",
    "latex-workshop.hover.ref.enabled": true,
    "latex-workshop.hover.preview.enabled": true,
    "latex-workshop.hover.preview.ref.enabled": true,
    "latex-workshop.hover.command.enabled": true,
    "latex-workshop.latex.recipe.default": "lastUsed",
    "latex-workshop.intellisense.citation.label": "title",
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.texdoc.path": "texdoc",
}
